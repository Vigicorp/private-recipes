{
    "manifests": {
        "vigicorp/theme-admin-bundle": {
            "manifest": {
                "bundles": {
                    "Vigicorp\\ThemeAdminBundle\\VigicorpThemeAdminBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "assets/": "assets/",
                    "config/": "%CONFIG_DIR%/",
                    "templates/": "templates/"
                },
                "composer-scripts": {
                    "sass:build": "symfony-cmd"
                },
                "add-lines": [
                    {
                        "file": "importmap.php",
                        "content": "    'admin' => [\n        'path' => './assets/js/admin.js',\n        'entrypoint' => true,\n    ],",
                        "position": "after_target",
                        "target": "return [",
                        "warn_if_missing": true
                    },
                    {
                        "file": "config/packages/security.yaml",
                        "content": "        # Change name if install on subfolder \n        vigicorp_theme_admin:\n            # Uncomment if install on subfolder\n            # pattern: ^(/admin|/vigicorp-datatable-bundle/vigicorp_theme_admin_)\n            lazy: true\n            provider: admin_provider\n            switch_user: { role: ROLE_ALLOWED_TO_SWITCH }\n            form_login:\n                login_path: vigicorp_theme_admin_login\n                check_path: vigicorp_theme_admin_login\n                default_target_path: vigicorp_theme_admin_default\n            logout:\n                path: vigicorp_theme_admin_logout\n                target: vigicorp_theme_admin_login\n                invalidate_session: false",
                        "position": "after_target",
                        "target": "security: false",
                        "warn_if_missing": true
                    },
                    {
                        "file": "config/packages/security.yaml",
                        "content": "        - { path: ^/connexion, roles: PUBLIC_ACCESS }\n        - { path: ^/deconnexion, roles: PUBLIC_ACCESS }\n        - { path: ^/mot-de-passe-oublie, roles: PUBLIC_ACCESS }\n        - { path: ^/, roles: IS_AUTHENTICATED }\n        # Replace with this if would install on subfolder admin\n        #- { path: ^/admin/connexion, roles: PUBLIC_ACCESS }\n        #- { path: ^/admin/deconnexion, roles: PUBLIC_ACCESS }\n        #- { path: ^/admin/mot-de-passe-oublie, roles: PUBLIC_ACCESS }\n        #- { path: ^/admin, roles: IS_AUTHENTICATED }",
                        "position": "after_target",
                        "target": "access_control:",
                        "warn_if_missing": true
                    }
                ]
            },
            "files": {
